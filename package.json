    "@hapi/h2o2": "^10.0.1",
    "@hapi/hapi": "^21.3.0",
    "@hapi/hoek": "^11.0.2",
    "@hapi/inert": "^7.0.1",
    "@hapi/scooter": "^7.0.0",
    "@hapi/vision": "^7.0.1",
    "@hapi/yar": "^11.0.1",
    "@joi/date": "^2.1.0",
    "accessible-autocomplete": "^2.0.4",
    "ajv": "^8.12.0",
    "app-module-path": "^2.2.0",
    "archiver": "^5.3.1",
    "blankie": "^5.0.0",
    "blipp": "^4.0.2",
    "comma-number": "^2.1.0",
    "csv-parse": "^5.3.6",
    "csv-stringify": "^6.3.0",
    "decimal.js-light": "^2.5.1",
    "dotenv": "^16.0.3",
    "es6-error": "^4.1.1",
    "file-type": "^12.4.2",
    "got": "^11.8.5",
    "govuk-frontend": "^3.14.0",
    "hapi-auth-cookie": "^9.1.0",
