    "shallow-diff": "^0.0.5",
    "url-join": "^4.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@hapi/code": "^9.0.3",
    "@hapi/lab": "^25.1.2",
    "axios": "^1.3.4",
    "cypress": "^8.7.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^1.6.2",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0",
    "pino-pretty": "^9.4.0",
    "sass": "^1.58.3",
    "sinon": "^15.0.1",
    "standard": "^17.0.0"
  },
  "standard": {
    "ignore": [
      "coverage",
      "data",
      "govuk_modules",
      "public",
      "cypress/fixtues/",
      "cypress/plugins/",
      "cypress/reports/",
      "cypress/screenshots/",
      "cypress/support/",
      "cypress/videos/",
      "cypress/temp/"
    ],
    "globals": [
      "cy",
      "Cypress",
      "describe",
      "expect",
      "it",
      "before",
      "beforeEach",
      "after",
      "afterEach"
    ]
  }
