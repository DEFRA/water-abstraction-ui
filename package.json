    "marked": "^4.2.12",
    "moment": "^2.29.4",
    "moment-range": "^4.0.2",
    "nearest-date": "^1.0.1",
    "nunjucks": "^3.2.3",
    "parse-domain": "^5.0.0",
    "path": "^0.12.7",
    "pluralize": "^8.0.0",
    "read-chunk": "^3.2.0",
    "request": "^2.88.2",
